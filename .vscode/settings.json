{
    "python.linting.pylintEnabled": true,
    "python.linting.flake8Enabled": true,
    "python.linting.mypyEnabled": true,
    "python.formatting.blackArgs": [ "--line-length=100" ],
    "python.formatting.blackPath": "${workspaceFolder}/.dev-3.11/bin/black",
    "python.formatting.provider": "black",
    "editor.formatOnSave": true,
    "python.testing.pytestArgs": [ "tests" ],
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "python.testing.pytestPath": "${workspaceFolder}/.dev-3.11/bin/pytest",
    "python.linting.flake8Args": [ "--max-line-length=100" ],
    "python.envFile": "${workspaceFolder}/.env",
    "python.linting.pylintPath": "${workspaceFolder}/.dev-3.11/bin/pylint",
    "python.defaultInterpreterPath": "${workspaceFolder}/.dev-3.11/bin/python",
    "python.pythonPath": "${workspaceFolder}/.dev-3.11/bin/python",
    "python.linting.flake8Path": "${workspaceFolder}/.dev-3.11/bin/flake8",
    "python.linting.mypyPath": "${workspaceFolder}/.dev-3.11/bin/mypy",
    "terminal.integrated.env.linux": { "PYTHONPATH": "${workspaceFolder}" },
    "json.format.enable": false
}